programa
{
	
	funcao inicio()
{
	inteiro distancia
	escreva("Qual a distancia a ser percorrida? : ")
	leia (distancia)
	
	se (distancia <=100){
	escreva ("Frente R$10,00")}
	
		
		
	senao se ((distancia >= 100) e (distancia<300) ){
	escreva ("Frente R$20,00")
	}
	
	senao se (distancia >=14){
	escreva ("Frente R$30,00")
	}
	
	
}
}