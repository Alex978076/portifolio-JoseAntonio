programa {
  funcao inicio() {
    inteiro alavanca
    escreva("Digite a posição da alavanca: ")
    leia(alavanca)
    escolha (alavanca){

      caso 1:
      escreva("Dinossauros")
      pare
      caso 2:
      escreva("Idade Média")
      pare
      caso 3:
      escreva("Cyberpunk 2077")
      pare
      caso contrario:
      escreva("ERROR 778")
    }
  }
}
