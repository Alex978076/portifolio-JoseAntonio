programa
{
	
funcao inicio()
{
		inteiro numero
		escreva("digite um numero: ")
		leia (numero)

		se(numero%5==0){

			escreva("Ele é multiplo")

		}
		senao{
			escreva ("Ele não é multiplo")

		
		}
		
	}
}

